{
  "name": "oblecto",
  "version": "0.0.2",
  "description": "Oblecto is a frontend agnostic media butler bringing high performance high quality media streaming to the home ",
  "author": "Robin <robin@mysunland.org>",
  "repository": "robinp7720/Oblecto",
  "private": false,
  "bin": {
    "oblecto": "./bin/oblecto.js"
  },
  "scripts": {
    "build:web": "cd Oblecto-Web; npm install; npm run build; cd ..;",
    "build": "./node_modules/@babel/cli/bin/babel.js src -d dist",
    "install": "npm run build && npm run build:web",
    "start": "./node_modules/@babel/node/bin/babel-node.js  src",
    "debug": "nodemon src --exec 'babel-node --inspect=0.0.0.0:9229 '",
    "pack": "webpack-cli",
    "compile": "nexe dist/Oblecto.js -t x86-8.0.0 -p /usr/bin/python2"
  },
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/polyfill": "^7.4.0",
    "@babel/preset-env": "^7.2.0",
    "async": "^2.6.1",
    "bcrypt": "^2.0.1",
    "epinfer": "^1.1.4",
    "fluent-ffmpeg": "^2.1.2",
    "guessit-exec": "0.0.1",
    "guessit-wrapper": "^1.0.1",
    "handbrake-js": "^3.0.8",
    "jsonwebtoken": "^8.4.0",
    "mdns": "^2.4.0",
    "mkdirp": "^0.5.1",
    "moviedb": "^0.2.10",
    "moviedb-promise": "^1.3.2",
    "mysql2": "^1.6.4",
    "node-tvdb": "^3.2.0",
    "promise-async": "^0.2.0",
    "recursive-readdir": "^2.2.2",
    "request": "^2.88.0",
    "restify": "^7.3.0",
    "restify-cors-middleware": "^1.1.1",
    "restify-errors": "^6.1.1",
    "sequelize": "^4.42.0",
    "socket.io": "^2.2.0",
    "which": "^1.3.1"
  },
  "devDependencies": {
    "@babel/node": "^7.2.2",
    "eslint": "^4.19.1",
    "jshint": "^2.9.7",
    "nexe": "^2.0.0-rc.34",
    "nodemon": "^1.18.9",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2"
  }
}
