    "epinfer": "^1.1.4",
    "jsonwebtoken": "^8.2.2",
    "mdns": "^2.4.0",
    "moviedb": "^0.2.10",
    "moviedb-promise": "^1.2.2",
    "mysql2": "^1.5.3",
    "node-tvdb": "^3.2.0",
    "promise-async": "^0.2.0",
    "recursive-readdir": "^2.2.2",
    "request": "^2.87.0",
    "restify": "^7.2.0",
    "restify-cors-middleware": "^1.1.0",
    "restify-errors": "^6.1.0",
    "sequelize": "^4.37.10",
    "socket.io": "^2.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
