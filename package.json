{
  "name": "oblecto",
  "version": "1.0.0",
  "description": "A free and opensource media server",
  "author": "Robin <robin@decker.cx>",
  "private": true,
  "scripts": {
    "start": "babel-node src",
    "debug": "nodemon src --exec 'babel-node --inspect '",
